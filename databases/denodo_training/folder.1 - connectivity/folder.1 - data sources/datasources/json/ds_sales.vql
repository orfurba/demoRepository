CREATE OR REPLACE DATASOURCE JSON ds_sales
    FOLDER = '/1 - connectivity/1 - data sources'
    ROUTE HTTP 'http.CommonsHttpClientConnection,120000' GET '${databases.denodo_training.folder.1 - connectivity.folder.1 - data sources.datasources.json.ds_sales.ROUTE.HTTP.URL}'
